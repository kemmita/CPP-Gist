#include <iostream>
#include <vector>
using namespace std;


int main(){
//below we are creating a 2 dimensional vector with 3 rows and 4 columns, the last number 7 can be any number, I just picked 7.
	vector< vector<int> > grid(3, vector<int>(4, 7));

//below is how you iterate through a 2 dimensional vector and display it. 
	for(int row=0; row<grid.size(); row++){
		for(int col=0; col<grid[row].size(); col++){
			cout<< grid[row][col]<<flush;
		}
		cout<<endl;
	}



	return 0;
}
